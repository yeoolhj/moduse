{
  "name": "moduse",
  "version": "0.1.2",
  "description": "modular framework",
  "type": "module",
  "main": "dist/lib/core/index.js",
  "module": "dist/es/core/index.js",
  "scripts": {
    "build": "tsc --outDir ./dist/lib && tsc -m es6 --outDir ./dist/es",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "framework",
    "modular",
    "typescript"
  ],
  "exports": {
    ".": {
      "types": "./dist/lib/core/index.d.ts",
      "import": "./dist/es/core/index.js",
      "require": "./dist/lib/core/index.js"
    },
    "./vue": {
      "types": "./dist/lib/vue/index.d.ts",
      "import": "./dist/es/vue/index.js",
      "require": "./dist/lib/vue/index.js"
    }
  },
  "files": [
    "dist/**/*"
  ],
  "author": "yeoolhj",
  "repository": {
    "type": "git",
    "url": "https://github.com/yeoolhj/moduse.git",
    "directory": "packages/moduse"
  },
  "bugs": "https://github.com/yeoolhj/moduse/issues",
  "license": "MIT"
}
